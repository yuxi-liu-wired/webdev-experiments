@article{hsiangQuantifyingInfluenceClimate2013,
  title = {Quantifying the {{Influence}} of {{Climate}} on {{Human Conflict}}},
  author = {Hsiang, Solomon M. and Burke, Marshall and Miguel, Edward},
  date = {2013-09-13},
  journaltitle = {Science},
  volume = {341},
  number = {6151},
  eprint = {24031020},
