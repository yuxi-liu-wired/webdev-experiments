@article{woodleyofmenieHowIntelligenceAffects2019,
  title = {How {{Intelligence Affects Fertility}} 30 {{Years On}}: {{Retherford}} and {{Sewell Revisited}} — {{With Polygenic Scores}} and {{Numbers}} of {{Grandchildren}}},
  shorttitle = {How {{Intelligence Affects Fertility}} 30 {{Years On}}},
  author = {Woodley of Menie, Michael A. and Rindermann, Heiner and Pallesen, Jonatan and Sarraf, Matthew A.},
