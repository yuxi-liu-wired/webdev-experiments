@article{dakosSlowingEarlyWarning2008,
  title = {Slowing down as an Early Warning Signal for Abrupt Climate Change},
  author = {Dakos, Vasilis and Scheffer, Marten and family=Nes, given=Egbert H., prefix=van, useprefix=false and Brovkin, Victor and Petoukhov, Vladimir and Held, Hermann},
