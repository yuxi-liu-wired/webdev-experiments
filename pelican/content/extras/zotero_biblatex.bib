  author = {Pathak, Jaideep and Lu, Zhixin and Hunt, Brian R. and Girvan, Michelle and Ott, Edward},
  date = {2017-12},
  journaltitle = {Chaos: An Interdisciplinary Journal of Nonlinear Science},
